<application>
  <component name="ActionMacroManager">
    <macro name="Run all tests in file">
      <action id="EditorTextEnd" />
      <action id="RunClass" />
      <action id="Back" />
    </macro>
    <macro name="block_paren">
      <typing text-keycode="0:1">(</typing>
      <action id="EditorLeft" />
      <action id="EditorDelete" />
      <action id="ReformatCode" />
    </macro>
    <macro name="&lt;noname&gt;">
      <action id="Find" />
      <shortuct text="ESCAPE" />
      <action id="EditorLeft" />
      <action id="ShowIntentionActions" />
      <shortuct text="ENTER" />
    </macro>
    <macro name="open_keyboard_shortcuts" />
  </component>
</application>